user_pass = input()
password = ''

for i in user_pass:
    if i == 'i':
        password += '!'
    elif i == 'a':
        password += '@'
    elif i == 'm':
        password += 'M'
    elif i == 'B':
        password += '8'
    elif i == 'o':
        password += '.'
    else:
        password += i

print(password + 'q*s')
