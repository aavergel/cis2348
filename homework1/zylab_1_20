userNum1 = int(input('Enter integer:\n'))

print('You entered:', userNum1)
print(userNum1, 'squared is', userNum1 * userNum1)
print('And', userNum1, 'cubed is', userNum1 * userNum1 * userNum1, '!!')

userNum2 = int(input('Enter another integer:\n'))

print(f'{userNum1} + {userNum2} is {userNum1 + userNum2}')
print(f'{userNum1} * {userNum2} is {userNum1 * userNum2}')
